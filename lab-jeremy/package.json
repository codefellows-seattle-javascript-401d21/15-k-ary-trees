{
  "name": "lab-15",
  "version": "1.0.0",
  "description": "binary tree html element text jest test",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "jest -i",
    "test:watch": "jest -i --watchAll",
    "test:debug": "DEBUG=http* jest -i",
    "start:watch": "nodemon index.js",
    "start:debug": "DEBUG=http* nodemon index.js"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "keywords": [
    "binary",
    "tree",
    "html",
    "element",
    "text",
    "jest",
    "test"
  ],
  "author": "Jeremy Pearson",
  "license": "MIT",
  "dependencies": {
    "bluebird": "^3.5.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "jest": "^22.1.4"
  }
}
